myHeaders.append("Content-Type", "application/json");

var raw = JSON.stringify({
  "name": "Onesies",
  "description": "Soft & comfortable one-piece bodysuits for babies",
  "categoryId": 2
});

var requestOptions = {
  method: 'PUT',
  headers: myHeaders,
  body: raw,
  redirect: 'follow'
};

fetch("http://localhost:8080/api/v1/subcategories/6", requestOptions)
  .then(response => response.text())
  .then(result => console.log(result))
  .catch(error => console.log('error', error));