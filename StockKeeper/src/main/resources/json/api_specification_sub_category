  var myHeaders = new Headers();
myHeaders.append("Authorization", "Bearer eyJhbGciOiJIUzUxMiJ9.eyJzdWIiOiJqb2huX2RvZSIsImV4cCI6MTY4Mjc5MjQ2MywiaWF0IjoxNjgyNzg4ODYzfQ.0iT0onMFmEK_UGcm-TC-BUhZYhBr-Lp8wwcYZyEQ-ERoWMBIpti25c3AbrlvV1t8VDxvjz2HKKXOURl6hFizZA");
myHeaders.append("Content-Type", "application/json");

var raw = JSON.stringify({
  "name": "Onesies",
  "description": "Soft and comfortable one-piece bodysuits for babies",
  "categoryId": 2
});

var requestOptions = {
  method: 'POST',
  headers: myHeaders,
  body: raw,
  redirect: 'follow'
};

fetch("http://localhost:8080/api/v1/subcategories", requestOptions)
  .then(response => response.text())
  .then(result => console.log(result))
  .catch(error => console.log('error', error));
  
  
